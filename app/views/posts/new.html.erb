<div class="container fadeUp">
  <div class="row">
    <div class="col-sm-4 img1">
    </div>

    <div class="col-sm-4">
      <div class="form-body p-4">
        <%= form_with model: @post, local:true do |f| %>

          <%= f.label :写真 %><br />
          <%= f.file_field :images, multiple: true %><br />
          <br />

          <%= f.label :タイトル %>
          <%= f.text_field :title, class:"form-control" %><br />

          <%= f.label :詳細 %>
          <%= f.text_area :body, class:"form-control", rows:"3" %><br />

          <%= f.submit '新規投稿', class:"btn btn-outline-dark mt-4" %>

        <% end %>
      </div>
    </div>

    <div class="col-sm-4 img2">
    </div>

  </div>
</div>